                android:background="#5fbcf1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:textColor="#ffffff"
                android:shadowColor="#090000" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnI"
                android:background="#648ab7"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:textColor="#ffffff"
                android:shadowColor="#090000"
                android:layout_above="@+id/btnM"
                android:layout_toEndOf="@+id/btnH"
                />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnJ"
                android:layout_below="@+id/btnE"
                android:layout_alignParentStart="true"
                android:background="#5fbcf1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:textColor="#ffffff"
                android:shadowColor="#090000" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnK"
                android:layout_alignTop="@+id/btnJ"
                android:layout_toEndOf="@+id/btnJ"
                android:background="#648ab7"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:textColor="#ffffff"
                android:shadowColor="#090000" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnL"
                android:layout_alignTop="@+id/btnK"
                android:layout_toEndOf="@+id/btnK"
                android:background="#5fbcf1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:textColor="#ffffff"
                android:shadowColor="#090000" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnLL"
                android:layout_alignTop="@+id/btnL"
                android:layout_toEndOf="@+id/btnL"
                android:background="#648ab7"
                android:layout_marginLeft="5dp"
