<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.self.trocket.tabtest2.MainActivity$PlaceholderFragment">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/imageView"
            android:src="@drawable/title"
            android:layout_above="@+id/btnB"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="5dp"
            android:layout_marginBottom="5dp"  />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnA"
                android:background="#5fbcf1"
                android:textColor="#ffffff"
                android:shadowColor="#090000"
                android:layout_alignParentTop="true"
                android:layout_toStartOf="@+id/btnK"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnB"
                android:layout_alignTop="@+id/btnA"
                android:layout_toEndOf="@+id/btnA"
                android:background="#648ab7"
                android:textColor="#ffffff"
                android:shadowColor="#090000"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnChe"
                android:layout_alignTop="@+id/btnB"
                android:layout_toEndOf="@+id/btnB"
                android:background="#5fbcf1"
                android:textColor="#ffffff"
                android:shadowColor="#090000"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnc"
                android:layout_alignTop="@+id/btnChe"
                android:layout_toEndOf="@+id/btnChe"
                android:background="#648ab7"
                android:textColor="#ffffff"
                android:shadowColor="#090000"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnD"
                android:clickable="false"
                android:background="#5fbcf1"
                android:textColor="#ffffff"
                android:shadowColor="#090000"
                android:layout_alignTop="@+id/btnc"
                android:layout_toEndOf="@+id/btnc"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnE"
                android:layout_below="@+id/btnA"
                android:layout_alignParentStart="true"
                android:background="#648ab7"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:textColor="#ffffff"
                android:shadowColor="#090000" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnF"
                android:layout_below="@+id/btnA"
                android:layout_toEndOf="@+id/btnA"
                android:background="#5fbcf1"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"  android:textColor="#ffffff"
                android:shadowColor="#090000"
                />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnG"
                android:layout_below="@+id/btnChe"
                android:layout_toEndOf="@+id/btnF"
                android:background="#648ab7"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:textColor="#ffffff"
                android:shadowColor="#090000"
                />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/btnH"
                android:layout_below="@+id/btnc"
                android:layout_toEndOf="@+id/btnG"
